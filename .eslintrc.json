{
  "env": {
    "es2021": true,
    "node": true,
    "mocha": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "func-names": 0,
    "consistent-return": 0,
    "no-console": 0,
    "import/no-import-module-exports": 0,
    "import/extensions": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0
  }
}
